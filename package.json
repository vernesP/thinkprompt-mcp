  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/vernes_p/thinkprompt-mcp.git"
  },
  "homepage": "https://bitbucket.org/vernes_p/thinkprompt-mcp#readme",
  "bugs": {
    "url": "https://bitbucket.org/vernes_p/thinkprompt-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "eslint": "^9.0.0"
  }
