{
  "name": "@honeyfield/thinkprompt-mcp",
  "version": "1.5.2",
  "description": "MCP Server for ThinkPrompt - Prompt Management System with Multi-Workspace Support",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "thinkprompt-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
